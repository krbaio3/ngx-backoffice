    "node-notifier": "^10.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "prettier-eslint": "12.0.0",
    "pretty-quick": "3.1.1",
    "protractor": "^7.0.0",
    "protractor-html-reporter-2": "^1.0.4",
    "rimraf": "3.0.2",
    "sort-package-json": "1.50.0",
    "standard-version": "9.3.0",
    "stylelint": "^13.13.1",
    "ts-node": "10.1.0",
    "typescript": ">=4.0 <4.2",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
